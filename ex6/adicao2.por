programa {
  funcao inicio() {
    inteiro num1, num2, soma

    escreva("Digite um número: ")
    leia(num1)
    limpa()

    escreva("Digite um número: ")
    leia(num2)
    limpa()

    soma = num1 + num2

    se (num1 + num2 > 20){
      escreva(soma+8)
    }

    senao{
      escreva(soma-5)
    }


    
    
  }
}
